	<body class="login-layout" style="background:#42adff;">
	<div class="center" style="margin-bottom: 32px;margin-top: 28px;">
		<h1><img src="img/logo.png"/></h1>
		<h1 style="color:#FFFFFF">Paparich后台管理系统</h1>
	</div>
		<div class="container-fluid" id="main-container">
			<div id="main-content">
				<div class="row-fluid">
					<div class="span12">
						
<div class="login-container">
<div class="row-fluid">
	
</div>
<div class="space-6"></div>
<div class="row-fluid">
<div class="position-relative">
	<div id="login-box" class="visible widget-box no-border">
		<div class="widget-body">
		 <div class="widget-main">
			<h4 class="header lighter bigger" style="font-family:"微软雅黑""><img src="img/input_info.png" style="margin-bottom:5px;"/> 请输入用户名和密码</h4>
			
			<div class="space-6"></div>
			
			<form action="${pageContext.request.contextPath}/login.do?method=login" method="post" id="loginActionUrl">
				 <fieldset>
					<label>
						<span class="block input-icon input-icon-right">
							<input type="text" class="span12" placeholder="Username" id="acount" name="acount" />
							<i class="icon-user"></i>
						</span>
					</label>
					<label>
						<span class="block input-icon input-icon-right">
							<input type="password" class="span12" placeholder="Password" id="pwd" name="pwd" />
							<i class="icon-lock"></i>
						</span>
					</label>
					
						<div class="qwqw"><p  style=" color:#f18382; font-size:12px;line-height:20px;">${msg }</p></div>
				
			
					<div class="row-fluid">
						<label class="span8">
						</label>
						<button  class="span4 btn btn-small btn-primary" type="submit"><i class="icon-key"></i> Login</button>
					</div>
					
				  </fieldset>
			</form>
		 </div><!--/widget-main-->
		
		
		 <div class="toolbar clearfix">
			<div style="height:43px;min-height43px; overflow:hidden; background:#fff">
				</div>
			

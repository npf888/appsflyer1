<!-- Modal 弹出框内容 begin -->
<div id="export_file" class="modal hide fade" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
  <div class="modal-header">
    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>
    <h3 id="myModalLabel">数据导出</h3>
  </div>
  <div class="modal-body">
   <form class="form-inline" action="userlogin.do?method=query_historyInfo" id="searchForm">		
         	
         	<div class="controls">
         	   <i class="icon-spinner icon-spin orange bigger-125" id="exporting"></i> <span id="exportLabel">正在生成文件....</span>
			</div>
			
			<br/>
			<div class="controls">
         		<span><a href="" id="fileName"></a></span>
         	</div>
		</form>
  </div>
  <div class="modal-footer">
    <button class="btn" data-dismiss="modal" aria-hidden="true">关闭</button>
  </div>
</div>

<!-- Modal 弹出框内容  end -->